   "source": [
    "# Decorators\n",
    "\n",
    "## Funções que recebem outras como parâmetros"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "# exemplo decorator\n",
    "def separar(funcao):\n",
    "    def wrapper():\n",
    "        print('-' * 30)\n",
    "        funcao()\n",
    "        print('-' * 30)\n",
    "    return wrapper"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "------------------------------\n",
      "Borges!\n",
      "------------------------------\n"
     ]
    }
   ],
   "source": [
    "@separar\n",
    "def say_hello():\n",
    "    print('Borges!')\n",
    "\n",
    "\n",
    "say_hello()"
   ]
  },
  {
